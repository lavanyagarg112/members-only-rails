<h1>Posts</h1>


<ul>
  <% @posts.each do |post| %>
    <li>
      <%= link_to post.title, post %>
    </li>
  <% end %>
</ul>
<%= link_to "New Post", new_post_path %>



<% if user_signed_in? %>
  <%= button_to 'Sign Out', destroy_user_session_path, method: :delete %>
  <% else %>
  <%= button_to 'Sign In', new_user_session_path, method: :get  %>
<% end %>
